<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    <title> Wedding Vendor &amp; Supplier Directory HTML Template - RealWed </title>
    <!-- Bootstrap -->
    <link href="~/theme/css/bootstrap.min.css" rel="stylesheet" />

    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Rubik:300,300i,400,400i,500,500i,700,700i,900,900i" rel="stylesheet">
    <!-- FontAwesome icon -->
    <link href="~/theme/fontawesome/css/fontawesome-all.css" rel="stylesheet" />
    <!-- Fontello icon -->
    <link href="~/theme/fontello/css/fontello.css" rel="stylesheet" />
    <!-- OwlCarosuel CSS -->
    <link href="~/theme/css/owl.carousel.css" rel="stylesheet" />
    <link href="~/theme/css/owl.theme.default.css" rel="stylesheet" />
    <!-- Favicon icon -->
    <link rel="shortcut icon" type="image/x-icon" href="~/theme/images/favicon.ico">
    <!-- Style CSS -->
    <link href="~/theme/css/style.css" rel="stylesheet" />
    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>

<body>
    <!-- header-top -->
    <partial name="_Topbar.cshtml" />
    <!--/.header-top -->
    <!-- header -->
    @(await Component.InvokeAsync("Header"))
<!-- /.header -->

    @RenderBody()
    <!-- footer-section -->
    @(await Component.InvokeAsync("Footer"))
    <!-- /.tiny-footer-section -->
    <a href="javascript:" id="return-to-top"><i class="fa fa-angle-up"></i></a>
    <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
    <script src="~/theme/js/jquery.min.js"></script>
    <!-- Include all compiled plugins (below), or include individual files as needed -->
    <script src="~/theme/js/bootstrap.min.js"></script>
    <script src="~/theme/js/menumaker.min.js"></script>
    <!-- owl-carousel js -->
    <script src="~/theme/js/owl.carousel.min.js"></script>
    <!-- nice-select js -->
    <script src="~/theme/js/jquery.nice-select.min.js"></script>
    <script src="~/theme/js/fastclick.js"></script>
    <script src="~/theme/js/custom-script.js"></script>
    <script src="~/theme/js/return-to-top.js"></script>
</body>

</html>